{
  "name": "twemoji-svelte-actions",
  "version": "1.0.0",
  "description": "Svelte wrapper for twemoji using actions",
  "main": " ",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc src/index.ts --outDir lib",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsraPerez98/twemoji-svelte-actions.git"
  },
  "keywords": [
    "twemoji",
    "svelte",
    "emoji",
    "emojis",
    "twitter"
  ],
  "author": "Israel Perez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IsraPerez98/twemoji-svelte-actions/issues"
  },
  "homepage": "https://github.com/IsraPerez98/twemoji-svelte-actions#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "twemoji": "^14.0.2"
  },
  "files": [
    "lib"
  ]
}
